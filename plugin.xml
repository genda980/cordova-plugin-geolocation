<?xml version='1.0' encoding='utf-8'?>
<plugin id="cordova-plugin-geolocation" version="1.0.0" xmlns="http://apache.org/cordova/ns/plugins/1.0"
        xmlns:android="http://schemas.android.com/apk/res/android">
    <name>Geolocation</name>
    <js-module name="Geolocation" src="www/Geolocation.js">
        <clobbers target="cordova.plugins.Geolocation"/>
    </js-module>
    <platform name="android">
        <config-file parent="/*" target="res/xml/config.xml">
            <feature name="Geolocation">
                <param name="android-package" value="cordova-plugin-geolocation.Geolocation"/>
            </feature>
        </config-file>
        <config-file parent="/*" target="AndroidManifest.xml"></config-file>
        <source-file src="src/android/Geolocation.java" target-dir="src/cordova-plugin-geolocation/Geolocation"/>
    </platform>
</plugin>